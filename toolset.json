{
  "schemaVersion": "1.0",
  "swiftCompiler": {
    "extraCLIOptions": [
      "-g",
      "-Onone",
      "-Xfrontend",
      "-disable-stack-protector",
      "-enable-experimental-feature",
      "Embedded",
      "-enable-experimental-feature",
      "Extern",
      "-Xclang-linker",
      "-nostdlib",
      "-Xfrontend",
      "-function-sections",
      "-Xfrontend",
      "-mergeable-symbols",
      "-Xfrontend",
      "-enable-single-module-llvm-emission",
      "-Xclang-linker",
      "-fuse-ld=lld"
    ]
  },
  "cCompiler": {
    "extraCLIOptions": [
      "-g",
      "-O0",
      "-nostdlib",
      "-ffunction-sections",
      "-mthumb",
      "-Werror",
      "-Wall",
      "-static",
      "-fno-stack-protector",
      "-fno-common"
    ]
  },
  "linker": {
    "extraCLIOptions": [
      "-nostdlib",
      "-t",
      "-v",
      "-T",
      "Sources/Support/linkerscript.ld",
      "-static",
      "-e",
      "vector_table",
      "--orphan-handling=error"
    ]
  }
}
